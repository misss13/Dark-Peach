Xcursor.theme: oreo_pink_cursors
Xcursor.size: 9
Xft.dpi:	96
Xft.antialias:	true
Xft.rgba:	rgb
Xft.hinting:	true
Xft.hintstyle:	hintslight
Xft.autohint:	false
Xft.lcdfilter:	lcddefault

!!URxvt.font: xft:3270Medium Nerd Font Mono Medium: pixelsize=15
URxvt.font: xft:Inconsolata for Powerline:pixelsize=15, xft:Iosevka Nerd    Font:pixelsize=15

!!URxvt.font:	xft:SauceCodePro Nerd Font Regular:pixelsize=15
URxvt.letterSpace: 0
URxvt.lineSpace: 0

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
Urxvt.scrollstyle: rxvt
URxvt.iso14755: true

!! Common Keybinds for Navigations
!!URxvt.perl-lib: ~/.config/urxvt-perls
URxvt.perl-ext-common:default,resize-font
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

!!scroll
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.C-M-c:    builtin-string:
URxvt.keysym.C-M-v:    builtin-string:
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard

URxvt.internalBorder: 26
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt*depth: 32
! special
*.foreground:	#ff3c79
URxvt*.background:	[93]#432222
*.cursorColor:	#ff3c79

! black
*.color0:	#72043d
*.color8:	#c02d67

! red
*.color1:	#72213c
*.color9:	#c0502d

! green
*.color2:	#834152
*.color10:	#f98361

! yellow
*.color3:	#b62b6e
*.color11:	#f93a3a

! blue
*.color4:	#8e2e2e
*.color12:	#f99977

! magneta
*.color5:	#813b3b
*.color13:	#ed7373

! cyan
